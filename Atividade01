        int idadeDois = 20;
        double altura = 1.73;
        boolean ehEstudante = true;

        int somaIdades = idade + idadeDois;
        double alturaDobrada = altura * 2;

        //resultados

        System.out.println("Idade: " + idade);
        System.out.println("Altura: " + altura);
        System.out.println("Nome: " + nome);
        System.out.println("Ã‰ estudante: " + ehEstudante);
        System.out.println("Soma de idades: " + somaIdades);
        System.out.println("Altura dobrada: " + alturaDobrada);

    }
}
